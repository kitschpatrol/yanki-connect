{
	"name": "yanki-connect",
	"version": "3.2.0",
	"description": "A fully-typed Anki-Connect API client.",
	"keywords": [
		"anki",
		"flashcards",
		"spaced-repetition",
		"anki-connect",
		"client",
		"api",
		"yanki",
		"npm-package"
	],
	"homepage": "https://github.com/kitschpatrol/yanki-connect",
	"bugs": "https://github.com/kitschpatrol/yanki-connect/issues",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/kitschpatrol/yanki-connect.git"
	},
	"license": "MIT",
	"author": {
		"name": "Eric Mika",
		"email": "eric@ericmika.com",
		"url": "https://ericmika.com"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"main": "./dist/index.js",
	"module": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"files": [
		"dist"
	],
	"scripts": {
		"build": "tsdown --minify --platform neutral --tsconfig tsconfig.build.json",
		"clean": "git rm -f pnpm-lock.yaml ; git clean -fdX",
		"coverage": "vitest run --coverage",
		"fix": "ksc fix",
		"lint": "ksc lint",
		"release": "bumpp --commit 'Release: %s' && pnpm run build && NPM_AUTH_TOKEN=$(op read 'op://Personal/npm/token') && pnpm publish",
		"test": "vitest"
	},
	"dependencies": {
		"@types/node": "^18.19.130",
		"open": "^10.2.0"
	},
	"devDependencies": {
		"@kitschpatrol/shared-config": "^5.8.3",
		"@types/jsdom": "^27.0.0",
		"@vitest/browser-playwright": "^4.0.8",
		"@vitest/coverage-v8": "^4.0.8",
		"bumpp": "^10.3.1",
		"jsdom": "^27.1.0",
		"playwright": "^1.56.1",
		"ts-morph": "^27.0.2",
		"tsdown": "^0.16.1",
		"typescript": "~5.9.3",
		"vitest": "^4.0.8"
	},
	"packageManager": "pnpm@10.20.0",
	"engines": {
		"node": "^18.15.0 || >=20.11.0"
	},
	"publishConfig": {
		"access": "public"
	}
}
